    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "~11.10.5",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.10",
