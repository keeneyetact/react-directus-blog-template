  "repository": {
    "type": "git",
    "url": "git+https://github.com/theluckyegg/stagcss.git",
    "directory": "packages/utilities"
  },
  "scripts": {
    "lint": "echo 'linting @package/web' && eslint .",
    "prettier": "echo 'formatting @package/web' && yarn prettier  --plugin-search-dir . --write . --check .",
    "test": "echo 'test @package/web' && yarn test:unit && yarn test:web",
    "test:unit": "echo 'test:unit @package/web' && vitest",
    "test:web": "echo 'test:web @package/web' && playwright test",
    "watch": "ts-node ./src/server.ts",
    "serve": "echo 'serving @package/web' && yarn watch",
    "build": "echo 'building @package/web' && tsc && vite build",
    "start": "echo 'starting @package/web' && cross-env NODE_ENV=production node ./dist/cjs/server.js"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "~1.2.0",
    "@emotion/react": "~11.10.5",
    "@emotion/styled": "~11.10.5",
